{
  "version": 2,
  "framework": "vanilla",
  "buildCommand": "echo 'build complete'",
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "functions": {
    "api/predict.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}