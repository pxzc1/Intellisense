{
  "version": 2,
  "functions": {
    "api/predict.py": {
      "runtime": "python3.9",
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}